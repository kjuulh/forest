  "query": "\n                SELECT\n                    r.id as release_id,\n                    r.release_intent_id,\n                    r.destination_id,\n                    r.status,\n                    d.name as destination\n                FROM releases r\n                JOIN destinations d ON r.destination_id = d.id\n                WHERE r.release_intent_id = $1\n            ",
